{
  "esversion": 11,
  "browser": true,
  "node": true,
  "strict": false,
  "undef": false,
  "unused": false,
  "predef": [
    "BigInteger",
    "randomStrings",
    "SHA256",
    "globalThis",
    "module",
    "exports",
    "require",
    "console",
    "CryptoJS",
    "random16byteHex",
    "define",
    "self"
  ],
  "maxerr": 200,
  "evil": true,
  "expr": true,
  "funcscope": false,
  "iterator": false,
  "lastsemic": true,
  "laxbreak": false,
  "laxcomma": false,
  "loopfunc": false,
  "multistr": false,
  "onecase": false,
  "proto": false,
  "regexdash": false,
  "scripturl": false,
  "smarttabs": false,
  "shadow": true,
  "sub": false,
  "supernew": false,
  "validthis": false
}